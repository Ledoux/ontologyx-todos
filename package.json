{
  "name": "ontologyx-todos",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "entitiex": "0.0.1",
    "enzyme": "^2.4.1",
    "filtex": "0.0.1",
    "lodash": "^4.11.2",
    "nodemon": "^1.11.0",
    "pluralize": "3.0.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-scripts": "^0.6.0",
    "svg-sprite": "^1.3.3",
    "transactionx-client": "0.0.1",
    "transactionx-express": "0.0.1"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "basscss": "^8.0.3",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.5.2",
    "redux-saga": "^0.12.0",
    "todomvc-app-css": "^2.0.6",
    "todomvc-common": "^1.0.2",
    "ttab": "^0.5.0"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "./node_modules/react-scripts/bin/react-scripts.js build",
    "console": "babel-node server/utils/consoleDatabase.js --presets es2015,stage-3",
    "eject": "./node_modules/react-scripts/bin/react-scripts.js eject",
    "postinstall": "npm run sprite-icons",
    "reset": "babel-node server/utils/resetDatabase.js --presets es2015,stage-3",
    "sprite-icons": "node_modules/svg-sprite/bin/svg-sprite.js -d --di --defs-dest=public/ --ds _svgs.html icons/*.svg",
    "start": "./node_modules/react-scripts/bin/react-scripts.js start",
    "test": "./node_modules/react-scripts/bin/react-scripts.js test --env=jsdom",
    "serve-dev": "nodemon server/index.js --exec babel-node --watch server --watch ../transactionx-express/lib --presets es2015,stage-3",
    "serve-prod": "node server/index.js"
  }
}
